{
  "version": 1,
  "paths": {
    "/cv-service/{user-id}/upload": {
      "name": "/cv-service/{user-id}/upload",
      "lambdaFunction": "cvServiceHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/cv-service/{user-id}": {
      "name": "/cv-service/{user-id}",
      "lambdaFunction": "cvServiceHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/cv-service/{user-id}/cv/{cv-id}": {
      "name": "/cv-service/{user-id}/cv/{cv-id}",
      "lambdaFunction": "cvServiceHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}
