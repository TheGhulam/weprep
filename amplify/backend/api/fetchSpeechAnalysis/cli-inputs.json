{
  "version": 1,
  "paths": {
    "/{user-id}/speech/analysis/{video-id}": {
      "name": "/{user-id}/speech/analysis/{video-id}",
      "lambdaFunction": "fetchUserSpeechAnalysisHandler",
      "permissions": {
        "setting": "open"
      }
    },
    "/{user-id}/speech/web-crawler-data/{video-id}": {
      "name": "/{user-id}/speech/web-crawler-data/{video-id}",
      "lambdaFunction": "fetchSpeechWebCrawlerDataHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}
