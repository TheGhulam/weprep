{
  "name": "weprep.web-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev",
    "dev:admin": "dotenv -- turbo dev --filter admin-frontend",
    "dev:app": "dotenv -- turbo dev --filter frontend",
    "lint": "turbo lint",
    "eslint": "eslint ./packages --ext .tsx",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf/renderer": "^3.4.4",
    "apexcharts": "^3.49.0",
    "pdf": "^0.1.0",
    "react-apexcharts": "^1.4.1",
    "@aws-amplify/adapter-nextjs": "^1.0.24",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/joy": "^5.0.0-beta.18",
    "@mui/material": "^5.15.14",
    "@mui/x-charts": "^7.3.0",
    "@tabler/icons": "^2.44.0",
    "@tabler/icons-react": "^2.44.0",
    "aws-amplify": "^6.0.24",
    "axios": "^1.6.8",
    "chart.js": "^4.4.2",
    "cheerio": "^1.0.0-rc.12",
    "jotai": "^2.5.1",
    "next": "^13.5.6",
    "next-themes": "^0.3.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^5.0.2",
    "react-github-calendar": "^4.1.3",
    "react-github-contribution-calendar": "^2.2.0",
    "react-material-ui-carousel": "^3.4.2",
    "react-pdf": "^7.7.1",
    "react-player": "^2.16.0",
    "react-router-dom": "^6.3.0",
    "react-webcam": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^16",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "turbo": "latest",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "globalDotEnv": [
    ".env"
  ],
  "pipeline": {
    "dev:admin": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "env": [
        "NEXT_PUBLIC_ADMIN_APIKEY",
        "NEXT_PUBLIC_API_URL"
      ]
    },
    "dev:app": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "env": [
        "NEXT_PUBLIC_ADMIN_APIKEY",
        "NEXT_PUBLIC_API_URL"
      ]
    },
    "dev": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "env": [
        "NEXT_PUBLIC_ADMIN_APIKEY",
        "NEXT_PUBLIC_API_URL"
      ]
    }
  }
}
